filters:
  and:
    - type.contains("class")
    - '!file.path.startsWith("Templates")'
views:
  - type: table
    name: Classes
    order:
      - file.name
    sort:
      - property: file.name
        direction: ASC
  - type: table
    name: LING
    filters:
      and:
        - file.name.startsWith("LING")
  - type: table
    name: CS
    filters:
      or:
        - file.hasLink("Notes/Computer Science")
        - file.name.startsWith("CS")
